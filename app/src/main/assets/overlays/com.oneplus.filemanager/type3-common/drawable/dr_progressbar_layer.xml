<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@*android:id/background">
        <shape>
            <gradient android:startColor="@*android:color/button_material_dark" android:endColor="@*android:color/button_material_dark" />
        </shape>
    </item>
    <item android:id="@*android:id/progress">
        <clip>
            <shape>
                <gradient android:startColor="@*android:color/accent_material_dark" android:endColor="@*android:color/accent_material_dark" />
            </shape>
        </clip>
    </item>
</layer-list>