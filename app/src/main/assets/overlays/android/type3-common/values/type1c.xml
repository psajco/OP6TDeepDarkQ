<?xml version="1.0" encoding="utf-8"?>
<resources>
	
    <color name="notification_action_color_filter">@*android:color/white</color>
    <color name="notification_media_primary_color">@*android:color/white</color>
    <color name="notification_media_secondary_color">@*android:color/secondary_text_material_dark</color>
    <color name="notification_mtrl_action_textcolor">@*android:color/white</color>
    <color name="notification_default_color">@*android:color/white</color>
    <color name="notification_icon_default_color">@*android:color/white</color>
    <color name="notification_action_tombstone_text_color">@*android:color/white</color>
    <color name="notification_text_default_color">@*android:color/white</color>
    <color name="notification_primary_text_color_dark">@*android:color/white</color>
    <color name="notification_primary_text_color_light">@*android:color/white</color>
    <color name="notification_secondary_text_color_dark">@*android:color/secondary_text_material_dark</color>
    <color name="notification_secondary_text_color_light">@*android:color/secondary_text_material_dark</color>
	
	
    <style name="NotificationPrimaryText">
        <item name="android:textColorPrimary">@*android:color/white</item>
        <item name="android:textColorSecondary">@*android:color/white</item>
        <item name="android:textColor">@*android:color/white</item>
    </style>
    <style name="NotificationSecondaryText">
        <item name="android:textColorPrimary">@*android:color/white</item>
        <item name="android:textColorSecondary">@*android:color/white</item>
        <item name="android:textColor">@*android:color/white</item>
    </style>
    <style name="notification_secondary_text">
        <item name="android:textColorPrimary">@*android:color/white</item>
        <item name="android:textColorSecondary">@*android:color/white</item>
        <item name="android:textColor">@*android:color/white</item>
    </style>
    <style name="TextAppearance.Material.Notification" parent="@*android:style/TextAppearance.Material">
        <item name="android:textSize">@*android:dimen/notification_text_size</item>
        <item name="android:textColor">@*android:color/white</item>
    </style>
    <style name="TextAppearance.Material.Notification.Emphasis" parent="@*android:style/TextAppearance.Material.Notification">
        <item name="android:textColor">@*android:color/white</item>
    </style>
    <style name="TextAppearance.Material.Notification.Info" parent="@*android:style/TextAppearance.Material.Notification">
        <item name="android:textSize">@*android:dimen/notification_subtext_size</item>
        <item name="android:textColor">@*android:color/white</item>
    </style>
    <style name="TextAppearance.Material.Notification.Title" parent="@*android:style/TextAppearance.Material.Notification">
        <item name="android:textSize">@*android:dimen/notification_title_text_size</item>
        <item name="android:textColor">@*android:color/white</item>
    </style>
</resources>