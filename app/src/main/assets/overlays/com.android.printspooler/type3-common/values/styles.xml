<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="PrintActivity" parent="@*android:style/Theme.DeviceDefault.Light">
        <item name="android:colorPrimary">@*android:color/background_material_dark</item>
        <item name="android:colorPrimaryDark">@*android:color/background_material_dark</item>
        <item name="android:colorAccent">@*android:color/accent_material_dark</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowBackground">@*android:color/background_material_dark</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>
	<style name="Theme.AddPrinterActivity" parent="@*android:style/Theme.DeviceDefault.Dialog">
        <item name="android:listSeparatorTextViewStyle">@*com.android.printspooler:style/ListSeparator</item>
        <item name="android:textAppearanceListItemSecondary">@*com.android.printspooler:style/ListItemSecondary</item>
    </style>
    <style name="Theme.SelectPrinterActivity" parent="@*android:style/Theme.DeviceDefault.Light">
        <item name="android:textAppearanceListItemSecondary">@*com.android.printspooler:style/ListItemSecondary</item>
    </style>
</resources>
