<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Theme.Settings.Home" parent="@*com.android.settings:style/Theme.Settings.HomeBase">
        <item name="android:colorBackground">@*android:color/background_dark</item>
        <item name="*com.android.settings:colorPrimary">@null</item>
        <item name="*com.android.settings:colorPrimaryDark">@null</item>
    </style>
    <style name="ThemeOverlay.SwitchBar.Settings" parent="@*com.android.settings:style/ThemeOverlay.SwitchBar.Settings.Base">
        <item name="android:textColorPrimaryInverse">@*android:color/white</item>
    </style>
    <style name="Widget.ActionBar.Base" parent="@*android:style/Widget.DeviceDefault.ActionBar.Solid">
        <item name="android:background">@*android:color/background_dark</item>
    </style>
    <style name="ThemeOverlay.Settings.SearchBar" parent="@*com.android.settings:style/Theme.Settings">
        <item name="android:colorBackground">@*android:color/background_dark</item>
        <item name="android:colorControlNormal">@*android:color/accent_material_dark</item>
        <item name="*com.android.settings:cardBackgroundColor">@*android:color/background_floating_material_dark</item>
    </style>
</resources>
