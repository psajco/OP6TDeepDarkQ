        <shape android:tint="@*android:color/background_material_dark" android:shape="rectangle">
            <solid android:color="@*android:color/white" />
        </shape>
    </item>
    <item android:gravity="fill_horizontal|bottom|center" android:id="@*com.oneplus.gallery:id/actionbar_divider">
        <shape android:tint="@*android:color/background_material_dark" android:shape="rectangle">
            <size android:height="@*com.oneplus.gallery:dimen/oneplus_contorl_divider_height_standard" />
            <solid android:color="@*android:color/white" />
        </shape>
    </item>
</layer-list>
